{
  "name": "philgo-chat",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bt": "ng build --prod --named-chunks=true",
    "s": "ionic s --no-open -- --disable-host-check",
    "cordova": "ionic cordova run android -l --address 192.168.0.254 --verbose",
    "serve:office": "ionic s --configuration=office -- --disable-host-check",
    "serve:local": "ionic serve --disable-host-check --configuration=local",
    "serve:live": "ionic serve --disable-host-check --configuration=live",
    "publish": "ionic build --prod && npm run scp",
    "scp": "cd www && scp -r * chat@v8.philgo.com:./www",
    "cordova:build": "rm -f philgo-chat.apk && ionic cordova build android --prod --release && printf 'asdf99' | jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore com.sonub.chat.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk sonubchat && /Users/jaehosong/Library/Android/sdk/build-tools/28.0.2/zipalign -v 4 platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk philgo-chat.apk",
    "cordova:build:install": "adb uninstall com.sonub.chat && npm run cordova:build && adb install philgo-chat.apk",
    "cordova:build:install:run": "npm run cordova:build:install && adb shell am start -n com.sonub.chat/com.sonub.chat.MainActivity",
    "cordova:change:icon": "ionic cordova resources -i && npm run cordova:build:install",
    "cordova:change:splash": "ionic cordova resources -s && npm run cordova:build:install:run"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "@ionic-native/camera": "5.0.0-beta.15",
    "@ionic-native/core": "5.0.0-beta.15",
    "@ionic-native/fcm": "^4.12.2",
    "@ionic-native/splash-screen": "5.0.0-beta.15",
    "@ionic-native/status-bar": "5.0.0-beta.15",
    "@ionic/angular": "4.0.0-beta.7",
    "@ionic/pro": "2.0.3",
    "bootstrap": "^4.1.3",
    "cordova-android": "7.0.0",
    "cordova-plugin-camera": "^2.4.1",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-fcm-with-dependecy-updated": "^2.2.4",
    "cordova-plugin-ionic-keyboard": "^2.1.2",
    "cordova-plugin-ionic-webview": "^2.1.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.3",
    "firebase": "^5.4.1",
    "rxjs": "6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.7.2",
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular-devkit/core": "~0.7.2",
    "@angular-devkit/schematics": "~0.7.2",
    "@angular/cli": "~6.1.1",
    "@angular/compiler": "^6.1.6",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@angularclass/hmr": "^2.1.3",
    "@ionic/ng-toolkit": "^1.0.0",
    "@ionic/schematics-angular": "^1.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.9.3",
    "codelyzer": "~4.4.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "description": "An Ionic project",
